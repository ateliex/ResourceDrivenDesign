<Page
    x:Class="Ateliex.Cadastro.Modelos.ModeloPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Ateliex.Cadastro.Modelos"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="ModeloPage"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Loaded="Page_Loaded"
    mc:Ignorable="d">

    <Grid>
        <StackPanel>
            <TextBlock Text="Código" />
            <TextBlock x:Name="nomeTextBlock" />
            <Button x:Name="detalhesButton" Click="detalhesButton_Click">Detalhes</Button>
        </StackPanel>
    </Grid>
</Page>
